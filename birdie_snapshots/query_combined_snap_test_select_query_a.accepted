SelectQuery(
  FromTable("cats"),
  [
    SelectString("name"),
    SelectString("age"),
  ],
  [],
  AndWhere([
    NoWherePart,
    OrWhere([
      WhereLowerOrEqual(
        WhereColumn("age"),
        WhereParam(IntParam(4)),
      ),
      WhereLike(
        WhereColumn("name"),
        "%ara%",
      ),
    ]),
  ]),
  NoLimitOffset,
  [],
  NoEpilogPart,
)