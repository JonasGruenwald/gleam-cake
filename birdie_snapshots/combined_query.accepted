        WhereLike(
          WhereColumn("name"),
          "%ara%",
        ),
      ]),
      NoGroupBy,
      NoWhere,
      [],
      NoLimitNoOffset,
      NoEpilog,
    ),
    Select(
      Selects([
        SelectColumn("name"),
        SelectColumn("age"),
      ]),
      FromTable("cats"),
      NoJoins,
      NotWhere(WhereIsNotBool(
        WhereColumn("is_wild"),
        False,
      )),
      NoGroupBy,
      NoWhere,
      [],
      NoLimitNoOffset,
      NoEpilog,
    ),
  ],
  LimitNoOffset(1),
  [OrderByColumn("age", Asc)],
  NoEpilog,
))