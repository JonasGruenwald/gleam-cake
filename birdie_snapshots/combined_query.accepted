CombinedQuery(Combined(
  UnionAll,
  [
    Select(
      Selects([
        SelectColumn("name"),
        SelectColumn("age"),
      ]),
      FromTable("cats"),
      NoJoins,
      OrWhere([
        WhereLowerOrEqual(
          WhereColumn("age"),
          WhereParam(IntParam(4)),
