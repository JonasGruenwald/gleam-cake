title: where_test
file: ./test/cake/internal/query/where_test.gleam
test_name: where_test
---
SelectQuery(Select(
  SelectAll,
  NoSelects,
  FromTable("cats"),
  NoJoins,
  AndWhere([
    WhereComparison(
      WhereColumn("age"),
      Equal,
      WhereParam(IntParam(10)),
    ),
    RawWhereFragment(FragmentLiteral("1 = 1")),
    WhereComparison(
      WhereParam(StringParam("Hello")),
      Equal,
      WhereColumn("name"),
    ),
    OrWhere([
      WhereIsBool(
        WhereColumn("is_wild"),
        False,
      ),
      WhereIsBool(
        WhereColumn("is_wild"),
        True,
      ),
    ]),
    WhereComparison(
      WhereColumn("age"),
      GreaterOrEqual,
      WhereParam(IntParam(0)),
    ),
  ]),
  NoGroupBy,
  NoWhere,
  NoOrderBy,
  NoLimit,
  NoOffset,
  NoEpilog,
))